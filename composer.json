{
    "name": "qbbr/tor-proxy",
    "type": "library",
    "description": "Simple PHP cURL wrapper for Tor network.",
    "keywords": ["tor", "curl", "proxy", "qbbr"],
    "homepage": "https://github.com/qbbr/TorProxy",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Sokolov Innokenty",
            "email": "imqbbr@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "QBBR\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload-dev": {
        "psr-4": {
            "QBBR\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.0.33"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
